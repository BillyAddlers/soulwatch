{
  "name": "@soulworker/core",
  "version": "1.0.0",
  "description": "A core app for a web game called Soulwatch.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "npm run lint && npm run compile",
    "compile": "tsc --build tsconfig.json",
    "lint": "eslint . --ignore-path .gitignore --ext .ts",
    "lint:fix": "eslint . --ignore-path .gitignore --ext .ts --fix",
    "start": "node --experimental-specifier-resolution=node dist",
    "start:dev": "node --no-warnings --experimental-specifier-resolution=node --loader ts-node/esm src"
  },
  "author": "Riichi_Rusdiana##6815",
  "license": "MIT",
  "dependencies": {
    "eiows": "^4.0.0",
    "express": "^4.17.3",
    "nuxt-socket-io": "^3.0.5",
    "socket.io": "^4.4.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.21",
    "ts-node": "^10.5.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.6",
    "utf-8-validate": "^5.0.9"
  }
}
